{
  "name": "serverlessrepo",
  "version": "0.0.1",
  "description": "",
  "module": "./dist/index.js",
  "main": "dist/src/functions/*.js",
  "private": true,
  "bin": {
    "cfa": "./dist/cli.js"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "docs"
  ],
  "featureApp": {
    "port": 3000,
    "excludeShares": []
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mtn-kente/serverlessrepo.git"
  },
  "keywords": [],
  "author": "Kente",
  "license": "UNLICENSED",
  "dependencies": {
    "@azure/functions": "^4.0.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "swagger-jsdoc": "^6.x",
    "swagger-ui-express": "^5.0.0",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.0",
    "@commitlint/config-conventional": "^17.8.1",
    "@commitlint/prompt-cli": "^18.6.1",
    "commitlint-plugin-function-rules": "^2.0.2",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "release": {
    "branches": [
      "release",
      "next"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "build",
              "release": "patch"
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "BREAKING CHANGE",
                "section": "ðŸ’¥ BREAKING CHANGES"
              },
              {
                "type": "feat",
                "section": "ðŸš€ Features"
              },
              {
                "type": "fix",
                "section": "ðŸ› Bugfixes"
              },
              {
                "type": "build",
                "section": "â™»ï¸ Updated dependencies"
              },
              {
                "type": "docs",
                "section": "ðŸ“ Docs"
              }
            ]
          }
        }
      ],
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/github",
        {
          "message": "chore(release): ${nextRelease.version}",
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ]
        }
      ],
      "@semantic-release/github"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "eslint --config .eslintrc.js --fix"
    ],
    "*.{ts,tsx,json,js,jsx,html,yml,md,mdx}": [
      "prettier --config .prettierrc.js --write"
    ]
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "directories": {
    "lib": "lib"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "start": "func start",
    "build": "tsc",
    "prestart": "npm run clean && npm run build",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest -c jest.unit.config.js",
    "test:integration": "jest -c jest.integration.config.js",
    "lint:check": "eslint --config .eslintrc.json --ext .ts,.tsx,.js,.jsx .",
    "lint:fix": "eslint --config .eslintrc.json --cache --fix --ext .ts,.tsx,.js,.jsx .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{ts,tsx,json,js,jsx,html,yml,md}\" \"!**/dist/**\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,js,jsx,html,yml,md}\" \"!**/dist/**\" \"!**/coverage/**\" \"!CHANGELOG.md\" \"!**/fa-cli-test/**\" \"!**/.github/workflows/**\" \"!**/fa-cli-test-npm/**\" \"!**/.nx/**\"",
    "prettier-watch": "onchange \"lib/**/*.ts\" \"test/**/*.ts\" -- prettier --write ",
    "clean": "npx rimraf dist",
    "semantic-release": "semantic-release",
    "commitlint": "commitlint --edit",
    "commit": "cz",
    "prepare": "husky install"
  }
}
